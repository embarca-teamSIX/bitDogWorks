case '#': 
  for (uint8_t l = 0; l < LEDS; l++) {
    digitalWrite(ledPins[l], LOW); // Desliga todos os LEDs
  }
  delay(500); // Atraso de meio segundo
  for (uint8_t l = 0; l < LEDS; l++) {
    digitalWrite(ledPins[l], HIGH); // Liga todos os LEDs
  }
  delay(500); // Atraso de meio segundo
  break;
